{
  "name": "Ember-cli Simple Table",
  "description": "Simple data table to try",
  "version": "0.0.6",
  "url": "https://github.com/chauhansudhir/ember-cli-simple-table",
  "options": {
    "enabledEnvironments": ["development", "production"],
    "paths": [
      "addon"
    ],
    "external": {
      "data": [{
        "base": "http://emberjs.com/api/",
        "json": "http://builds.emberjs.com/tags/v2.0.0/ember-docs.json"
      }]
    },
    "themedir": "node_modules/yuidoc-ember-theme",
    "helpers": ["node_modules/yuidoc-ember-theme/helpers/helpers.js"],
    "exclude": "vendor",
    "outdir": "docs",
    "linkNatives": true,
    "quiet": true,
    "parseOnly": false,
    "lint": false
  }
}
